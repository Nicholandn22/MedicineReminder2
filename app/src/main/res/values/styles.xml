<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="DataLabel">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="DataValue">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="DataValueBold">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="ObatButton">
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:radius">16dp</item>
    </style>

    <style name="ButtonRounded">
        <item name="android:layout_marginHorizontal">8dp</item>
        <item name="android:radius">16dp</item>
        <item name="android:paddingHorizontal">32dp</item>
    </style>

</resources>